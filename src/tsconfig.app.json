{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "types": [],
    "paths": {
      "@config/*": ["config/*"],
      "@components/*": ["app/components/*"],
      "@directives/*": ["app/directives/*"],
      "@guards/*": ["app/guards/*"],
      "@interceptors/*": ["app/interceptors/*"],
      "@interfaces/*": ["app/interfaces/*"],
      "@models/*": ["app/models/*"],
      "@modules/*": ["app/modules/*"],
      "@pipes/*": ["app/pipes/*"],
      "@resolvers/*": ["app/resolvers/*"],
      "@services/*": ["app/services/*"]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
